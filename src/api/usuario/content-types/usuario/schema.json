{
  "kind": "collectionType",
  "collectionName": "usuarios",
  "info": {
    "singularName": "usuario",
    "pluralName": "usuarios",
    "displayName": "Usuario"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Nombre": {
      "type": "string"
    },
    "Apellidos": {
      "type": "string"
    },
    "Email": {
      "type": "email"
    },
    "Rol": {
      "type": "enumeration",
      "enum": [
        "jugador",
        "entrenador",
        "gestor"
      ]
    },
    "ImagenPerfil": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "entrenamientos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::entrenamiento.entrenamiento",
      "mappedBy": "jugadores"
    },
    "entrenamientosAsignadoss": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::entrenamiento.entrenamiento",
      "mappedBy": "entrenador"
    }
  }
}
