{
  "kind": "collectionType",
  "collectionName": "entrenamientos",
  "info": {
    "singularName": "entrenamiento",
    "pluralName": "entrenamientos",
    "displayName": "Entrenamiento",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Nombre": {
      "type": "string"
    },
    "Descripcion": {
      "type": "text"
    },
    "FechaHora": {
      "type": "datetime"
    },
    "Latitud": {
      "type": "float"
    },
    "Longitud": {
      "type": "float"
    },
    "TipoEntrenamiento": {
      "type": "enumeration",
      "enum": [
        "Fuerza",
        "Resistencia",
        "Velocidad",
        "Habilidad",
        "Mecanica",
        "Tecnica",
        "Flexibilidad",
        "Coordinacion",
        "Potencia"
      ]
    },
    "Confirmaciones": {
      "type": "json"
    },
    "gestor": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::usuario.usuario",
      "inversedBy": "entrenamientos"
    },
    "sesiones": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sesion.sesion",
      "mappedBy": "entrenamiento"
    },
    "jugadoresAsitentes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::usuario.usuario",
      "inversedBy": "entrenamientosConfirmados"
    },
    "entrenadores": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::usuario.usuario",
      "inversedBy": "entrenamientosVisto"
    },
    "ImagenEntreno": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
